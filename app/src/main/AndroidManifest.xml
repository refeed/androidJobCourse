<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.refeed_ppb1.jobcourse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JobCourse">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GetStartedActivity"></activity>
        <activity android:name=".RegisterOneActivity"></activity>
        <activity android:name=".RegisterTwoActivity"></activity>
        <activity android:name=".SignInActivity"></activity>
        <activity android:name=".SuccessRegisterActivity"></activity>
    </application>

</manifest>